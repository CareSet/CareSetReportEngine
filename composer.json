{
    "name": "careset/caresetjwtauthclient",
    "require": {
        "php": ">=7.0.0",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4" : {
            "CareSet\\CareSetJWTAuthClient\\" : "src/"
        }
    },
    "extra": {
         "branch-alias": {
            "dev-develop": "1.0-dev"
        },
	    "laravel": {
	        "providers": [
	            "CareSet\\CareSetJWTAuthClient\\ServiceProvider"
	        ]
	    }
	},
     "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=CareSet\\CareSetJWTAuthClient\\ServiceProvider"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --provider=CareSet\\CareSetJWTAuthClient\\ServiceProvider"
        ],
    }
}
